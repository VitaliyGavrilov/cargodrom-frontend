{
  "name": "cargodrom-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:port": "ng serve --port 3434",
    "build": "ng build --configuration development --base-href /cargodrom-frontend",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "api": "ng-openapi-gen -i https://dev.cargodrom.com/openapi2.php",
    "api:dev": "ng-openapi-gen -i https://dev.cargodrom.com/openapi2.php",
    "api:prod": "ng-openapi-gen -i https://cargodrom.com/openapi2.php",
    "build:github": "ng build --configuration development --base-href https://TriviumStudio.github.io/cargodrom-frontend/",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "build-prod_in_crm": "ng build --configuration production --base-href /crm/ --output-path dist/crm",
    "build_test_prod": "ng build --configuration production --base-href /prod/ --output-path dist/test-prod",
    "build_test_dev": "ng build --configuration development --base-href /dev/ --output-path dist/test-dev",
    "build_test_both": "npm run build_test_prod && npm run build_test_dev"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "~16.2.14",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/material": "~16.2.14",
    "@angular/material-moment-adapter": "^16.2.14",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@ng-select/ng-select": "^11.2.0",
    "moment": "^2.30.1",
    "ngx-color": "^9.0.0",
    "ngx-color-picker": "^15.0.0",
    "ngx-mask": "^15.0.0",
    "ngx-scrollbar": "^12.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.16",
    "@angular/cli": "~16.2.16",
    "@angular/compiler-cli": "^16.2.12",
    "@angular/localize": "^16.2.12",
    "@types/jasmine": "~4.3.4",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-openapi-gen": "^0.25.1",
    "typescript": "~4.9.5"
  }
}
